	<% provide(:title, 'Users') %>
<p id="notice"><%= notice %></p>

<h1>Listing Users</h1>

<table>
  <thead>
    <tr>
      
      <th>Name</th>
      <th>Email</th>
      <th>Location</th>
      <th>Foodtype</th>
      <th colspan="3"></th>
      
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.name %></td>
        <td><%= user.email %></td>
        <td><%= user.location %></td>
        <td><%= user.foodtype %></td>
        
      </tr>
    <% end %>
  </tbody>
</table>

<br>




<%= link_to 'New User', new_user_path %>

